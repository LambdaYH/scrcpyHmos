{
  "module": {
    "name": "app",
    "type": "entry",
    "description": "$string:module_desc",
    "mainElement": "ScrcpyHmosAbility",
    "deviceTypes": [
      "phone",
      "tablet"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:main_pages",
    "requestPermissions": [
      {
        "name": "ohos.permission.INTERNET",
        "reason": "$string:permission_internet_reason",
        "usedScene": {
          "abilities": [
            "ScrcpyHmosAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.GET_NETWORK_INFO",
        "reason": "$string:permission_network_info_reason",
        "usedScene": {
          "abilities": [
            "ScrcpyHmosAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.MICROPHONE",
        "reason": "$string:permission_microphone_reason",
        "usedScene": {
          "abilities": [
            "ScrcpyHmosAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.READ_PASTEBOARD",
        "reason": "$string:permission_read_pasteboard_reason",
        "usedScene": {
          "abilities": [
            "ScrcpyHmosAbility"
          ],
          "when": "inuse"
        }
      }
    ],
    "abilities": [
      {
        "name": "ScrcpyHmosAbility",
        "srcEntry": "./ets/scrcpyhmosability/ScrcpyHmosAbility.ets",
        "description": "$string:ScrcpyHmosAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:ScrcpyHmosAbility_label",
        "startWindowIcon": "$media:icon_startwindow",
        "startWindowBackground": "$color:start_window_background",
        "exported": true,
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "ohos.want.action.home"
            ]
          }
        ]
      }
    ],
    "extensionAbilities": [
      {
        "name": "ScrcpyHmosBackupAbility",
        "srcEntry": "./ets/scrcpyhmosbackupability/ScrcpyHmosBackupAbility.ets",
        "type": "backup",
        "exported": false,
        "metadata": [
          {
            "name": "ohos.extension.backup",
            "resource": "$profile:backup_config"
          }
        ],
      }
    ]
  }
}